{
  "name": "self-storybook",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "build-storybook-vercel": "yarn install-sdk && storybook build",
    "ci": "yarn nice && yarn types && yarn test:run",
    "dev": "vite",
    "format": "biome format --write .",
    "install-sdk": "bash scripts/install-sdk.sh && yarn install",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "nice": "biome check --write --unsafe .",
    "preview": "vite preview",
    "preview-storybook": "yarn build-storybook && npx serve storybook-static",
    "storybook": "storybook dev -p 6006",
    "storybook:fresh": "rm -rf storybook-static node_modules/.cache/storybook && storybook dev -p 6006",
    "test": "NODE_OPTIONS='--experimental-json-modules' vitest",
    "test:coverage": "NODE_OPTIONS='--experimental-json-modules' vitest run --coverage",
    "test:run": "NODE_OPTIONS='--experimental-json-modules' vitest run",
    "test:vercel": "rm -rf storybook-static .local-packages && bash scripts/install-sdk.sh && yarn install && NODE_ENV=production STORYBOOK_DISABLE_TELEMETRY=1 yarn build-storybook",
    "types": "tsc -b",
    "validate": "yarn nice && yarn test:run"
  },
  "resolutions": {
    "node-forge": "^1.3.1"
  },
  "dependencies": {
    "@selfxyz/common": "^0.0.9",
    "@selfxyz/mobile-sdk-alpha": "file:.local-packages/mobile-sdk-alpha",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-native-localize": "^3.2.1",
    "react-native-svg": "^15.14.0",
    "react-native-svg-circle-country-flags": "^0.2.2",
    "react-native-web": "^0.21.1",
    "tamagui": "1.126.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@storybook/test": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/react-native": "^0.73.0",
    "@types/react-native-web": "^0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "buffer": "^6.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "playwright": "^1.56.0",
    "process": "^0.11.10",
    "react-native": "^0.82.0",
    "storybook": "^9.1.10",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
